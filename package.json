{
  "name": "navaa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "playwright test --reporter=list --timeout=30000 || echo \"No tests yet\"",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "prepare": "husky install",
    "typegen": "supabase gen types typescript --schema public > src/types/supabase.ts",
    "test:decision-review": "tsx scripts/decisionReviewSmokeTest.ts"
  },
  "dependencies": {
    "@sentry/nextjs": "^8.17.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.5.2",
    "@types/react-dom": "^18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lint-staged": "^16.2.1",
    "next": "^15.5.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@types/react": "^18",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9",
    "eslint-config-next": "^15.5.9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
